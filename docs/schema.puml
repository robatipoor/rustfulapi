entity "**_sqlx_migrations**" {
  + ""version"": //bigint [PK]//
  --
  *""description"": //text //
  *""installed_on"": //timestamp with time zone //
  *""success"": //boolean //
  *""checksum"": //bytea //
  *""execution_time"": //bigint //
}

entity "**file**" {
